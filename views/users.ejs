<table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Username</th>
        </tr>
    </thead>
    <tbody>
            <% users.forEach(user => {%>
                <tr>
                <td><%= user.firstName %></td>  
                <td><%= user.lastName %></td>
                <td><%= user.email %></td>  
                <td><%= user.username %></td>
                </tr>
            <% })%>
    </tbody>
</table>

