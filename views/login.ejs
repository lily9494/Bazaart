<header><%- include("./partials/navLogin") %></header>
<form action="/home" method="post">
 <div class="row">
  <div class="col s12 m8 l4 offset-m2 offset-l4">
    <div class="card">
      <div class="card-action teal lighten-1 white-text">
        <h3>Login Form</h3>
      </div>
      <div class="card-content">
        <div class="form-field">
             <label for="email">Email</label> 
             <input type="email" name="email" placeholder="Email" class="validate"  required=""  aria-required="true">
        </div>
        <div class="form-field">
             <label for="password">Password</label>
             <input type="password" name="password" placeholder="password" minlength=10 class="validate"  required=""  aria-required="true"> 
        </div>
        <div class="form-field">
             <button class="btn waves-effect waves-light" type="submit" name="action">Login
             <i class="material-icons right">send</i>
            </button> 
        </div>
       </div>
   </div>
 </div>
</form>
